
#!/bin/sh
echo Generated automatically by geninstall.awk at:2011 Mon February-28 15:52:23
echo Installing CVORA driver...
echo 8 > /proc/sys/kernel/printk

/sbin/insmod cvora.ko dname="cvora" luns= vme1= win1=0x amd1= dwd1=
MAJOR=`cat /proc/devices | awk '{if ($2 == "cvora") printf $1}'`
echo Making nodes for ${MODULE} major device cvora ${MAJOR}
if [ -z "$MAJOR" ]; then
     echo "driver CVORA not installed !"
     exit 2
fi

rm -f /dev/cvora.*

